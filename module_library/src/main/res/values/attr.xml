<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LyProcessView">
        <attr name="content_color" format="color" />
        <attr name="frame_color" format="color"/>

        <attr name="bar_width" format="dimension" />
        <attr name="bar_height" format="dimension"/>
        <attr name="process" format="integer"/>

    </declare-styleable>


    <declare-styleable name="LyWaveView">

        <attr name="front_wave_color" format="color" />
        <attr name="back_wave_color" format="color" />
        <attr name="circle_color" format="color" />

        <attr name="wave_width" format="dimension" />
        <attr name="wave_height" format="dimension" />

    </declare-styleable>

</resources>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiyoumobile.module.library">

    <application
        android:name="debug.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="debug.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".l_common.ui.LyLoginActivity"/>
        <activity android:name=".l_common.ui.LyDetailActivity" />

        <activity android:name=".l_achieve.ui.LyAchieveActivity"/>
        <activity android:name=".l_search.ui.LySearchActivity" />

        <activity android:name=".l_distribution.ui.LyDistributionActivity" />
        <activity android:name=".l_schedule.ui.LyOpenTimeActivity"/>

        <activity android:name=".l_history.ui.LyHistoryActivity"/>
        <activity android:name=".l_renew.ui.LyRenewListActivity"/>
        <activity android:name=".l_search.ui.LySearchListActivity"/>
        <activity android:name=".l_borrow.ui.LyBorrowListActivity"/>

        <meta-data
            android:name="design_width"
            android:value="720" />
        <meta-data
            android:name="design_height"
            android:value="1280" />

    </application>

</manifest>
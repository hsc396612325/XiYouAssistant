<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiyoumobile.module.library">

    <application
        android:name="debug.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="debug.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="design_width"
            android:value="720" />
        <meta-data
            android:name="design_height"
            android:value="1280" />

        <activity android:name=".ui.activity.LySearchActivity" >
        </activity>
        <activity android:name=".ui.activity.LyAchieveActivity">
        </activity>
        <activity android:name=".ui.activity.LyListActivity" >
        </activity>
        <activity android:name=".ui.activity.LyDistributionActivity" >
        </activity>
        <activity android:name=".ui.activity.LyDetailActivity" >
        </activity>
        <activity android:name=".ui.activity.LyOpenTimeActivity">
        </activity>
        <activity android:name=".ui.activity.LibraryLoginActivity">
        </activity>
    </application>

</manifest>